{
  "name": "projet-master",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "db:migrate": "set TS_NODE_COMPILER_OPTIONS='{\"module\":\"commonjs\"}' && knex migrate:latest",
    "db:migrate:undo": "set TS_NODE_COMPILER_OPTIONS='{\"module\":\"commonjs\"}' && knex migrate:down",
    "db:migrate:make": "set TS_NODE_COMPILER_OPTIONS='{\"module\":\"commonjs\"}' && knex migrate:make",
    "vercel-build": "prisma generate && npm db:migrate && next build"
  },
  "dependencies": {
    "@prisma/client": "^5.8.1",
    "@vercel/postgres": "^0.5.1",
    "dotenv": "^16.4.1",
    "knex": "^3.1.0",
    "next": "14.0.3",
    "pg": "^8.11.3",
    "react": "^18",
    "react-dom": "^18",
    "ts-node": "^10.9.2"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.3.1",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.0.1",
    "eslint": "^8",
    "eslint-config-next": "14.0.3",
    "postcss": "^8",
    "prisma": "^5.8.1",
    "tailwindcss": "^3.3.0",
    "typescript": "^5"
  }
}
